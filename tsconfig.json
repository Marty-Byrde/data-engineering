{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "stripInternal": true,
    "noImplicitAny": true,
  },
  "exclude": [
    "node_modules",
    "dist",
    "docs"
  ]
}